<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tourguide">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/tourguideappicon_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/tourguideappicon_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PlacesActivity" android:label="@string/places" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".RestaurantsActivity" android:label="@string/restaurants" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".EventsActivity" android:label="@string/events" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".MiscellaneousActivity" android:label="@string/miscellaneous" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>